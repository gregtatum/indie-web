{
  "name": "js-template",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "./bin/build.sh",
    "check-updates": "npx npm-check-updates",
    "start-ts": "tsc --noEmit --watch",
    "start-webpack": "NODE_ENV=development SITE=browserchords ./bin/server.js",
    "start-webpack-floppy": "NODE_ENV=development SITE=floppydisk ./bin/server.js",
    "start": "run-p start-ts start-webpack",
    "start-floppy": "run-p start-ts start-webpack-floppy",
    "lint": "eslint '*/**/*.{js,ts,tsx}'",
    "bundle-size": "./bin/bundle-size.sh",
    "ts": "tsc --noEmit",
    "test": "env NODE_ENV=test jest",
    "test-coverage": "env NODE_ENV=test jest --coverage && echo 'View coverage report at: ./coverage/lcov-report/'",
    "test-all": "run-s lint ts test"
  },
  "alias": {
    "src": "./src"
  },
  "author": "Greg Tatum",
  "browserslist": [
    "since 2019-01"
  ],
  "jest": {
    "prettierPath": "./node_modules/prettier-2/index.js",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!**/node_modules/**"
    ],
    "testEnvironment": "./src/test/utils/fix-jsdom.ts",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "transformIgnorePatterns": ["/node_modules/(?!${esModules})"],
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/src/test/__mocks__/style.ts",
      "^nanoid(/(.*)|$)": "nanoid$1"
    },
    "roots": [
      "src/test"
    ],
    "setupFiles": [
      "./src/test/utils/setupFiles.ts"
    ],
    "setupFilesAfterEnv": [
      "./src/test/utils/setupAfterEnv.ts"
    ],
    "verbose": true
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@codemirror/view": "^6.22.3",
    "@lezer/generator": "^1.5.1",
    "@lezer/lr": "^1.3.14",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/common-tags": "^1.8.4",
    "@types/jest": "^29.5.11",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "blob-polyfill": "^7.0.20220408",
    "common-tags": "^1.8.2",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.1",
    "eslint-plugin-react": "^7.33.2",
    "fake-indexeddb": "^5.0.1",
    "fetch-mock-jest": "^1.5.1",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "node-polyfill-webpack-plugin": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.1",
    "prettier-2": "npm:prettier@^2",
    "style-loader": "^3.3.3",
    "typescript": "^5.2.0",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "workbox-webpack-plugin": "^7.0.0"
  },
  "dependencies": {
    "hunspell-asm": "github:gregtatum/hunspell-asm#72494a846ea709815b0bebf037f59853912dd4a3",
    "@codemirror/commands": "^6.3.2",
    "@codemirror/lang-markdown": "^6.2.3",
    "@codemirror/language": "^6.9.3",
    "@codemirror/language-data": "^6.3.1",
    "@codemirror/search": "^6.5.5",
    "@codemirror/state": "^6.3.3",
    "@types/codemirror": "^5.60.15",
    "@types/node": "^20.10.5",
    "codemirror": "^6.0.1",
    "codemirror-lang-chordpro": "^0.0.6",
    "dropbox": "^10.34.0",
    "idb": "^8.0.0",
    "marked": "^11.1.0",
    "pdfjs-dist": "^4.0.269",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.0.4",
    "react-router-dom": "^6.21.1",
    "redux": "^5.0.1",
    "redux-thunk": "^3.1.0",
    "turndown": "^7.1.2",
    "uuid": "^9.0.1"
  }
}
