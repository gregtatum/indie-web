  "jest": {
    "prettierPath": "./node_modules/prettier-2/index.js",
    "collectCoverageFrom": [
      "src/frontend/**/*.{js,jsx,ts,tsx}",
      "!**/node_modules/**"
    ],
    "testEnvironment": "./src/frontend/test/utils/fix-jsdom.ts",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!${esModules})"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/src/frontend/test/__mocks__/style.ts",
      "^nanoid(/(.*)|$)": "nanoid$1"
    },
    "roots": [
      "src/frontend/test"
    ],
    "setupFiles": [
      "./src/frontend/test/utils/setupFiles.ts"
    ],
    "setupFilesAfterEnv": [
      "./src/frontend/test/utils/setupAfterEnv.ts"
    ],
    "verbose": true
