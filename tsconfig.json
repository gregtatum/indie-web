{
  "compilerOptions": {
    "target": "esnext",
    "jsx": "react",
    "module": "commonjs",
    "strict": true,
    "baseUrl": "./src",
    "noEmit": true,
    "allowJs": true,
    "checkJs": true,
    "lib": ["esnext"],
    "paths": {
      "frontend/*": ["./frontend/*"],
      "server/*": ["./server/*"],
      "shared/*": ["./shared/*"],
    }
  },
  "include": ["./src"]
}
